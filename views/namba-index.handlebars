<div class="row">
    <div class="col-sm-8 col-sm-offset-2">
        <ul>
            {{#each nambas}}
                <li class="list-group-item">
                    <div class="lead"><a href="/nambas/{{this._id}}">{{this.title}}</a></div>
                    <a href="{{this.url}}" target="_blank">{{this.url}}</a>
                    <div class="text-right">
                    <a href="/n/{{this.subnamba}}">{{this.subnamba}}</a>
                    </div>
                </li>
            {{/each}}
        </ul>
    </div>
</div>